		<setting name="useColumnLabel" value="true" />
		<setting name="defaultExecutorType" value="REUSE" />
		<setting name="defaultStatementTimeout" value="25000" />
	</settings>
	<typeAliases>
		<typeAlias alias="Users" type="cn.com.cowboy.project.entity.Users" />
		<typeAlias alias="Department" type="cn.com.cowboy.project.entity.Department" />
		<typeAlias alias="TransBill" type="cn.com.cowboy.project.entity.TransBill" />
		<typeAlias alias="TransBillFlow" type="cn.com.cowboy.project.entity.TransBillFlow" />
	</typeAliases>
